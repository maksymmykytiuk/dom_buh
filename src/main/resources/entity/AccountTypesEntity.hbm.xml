<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.AccountTypesEntity" table="account_types" schema="dom_buh">
        <id name="idTypes">
            <column name="id_types" sql-type="int(11)"/>
        </id>
        <property name="typeName">
            <column name="type_name" sql-type="varchar(45)" length="45"/>
        </property>
    </class>
</hibernate-mapping>